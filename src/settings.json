{
    "colors":
    {
        "primary":"#3498db",
        "secondary":"rgb(60, 55, 50)",
        "lighter":"#fff",
        "light":"#f4f4f4",
        "grey":"rgb(116, 118, 120)",
        "yellow":"#f1c40f",
        "backgroundPage":"__%colors,light%",
        "strawberry":"rgb(205, 0, 55)",
        "red":"#c0392b",
        "blue":"rgb(0, 136, 206)",
        "green":"rgb(130, 190, 0)"
    },
    "text":
    {
        "defaultSize":"14px",
        "title1":"30px",
        "title2":"25px",
        "title3":"20px"
    },
    "grid":
    {
